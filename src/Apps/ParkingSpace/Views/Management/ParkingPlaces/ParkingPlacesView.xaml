<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ParkingSpace.Views.ParkingPlacesView">
  <ContentPage.Content>
    <Grid>
      <ListView x:Name="ListView"
                  CachingStrategy="RecycleElement"
                  Background="White"
                  SelectionMode="None"
                  >

        <ListView.ItemTemplate>
          <DataTemplate>
            <ViewCell>
              <Label Text="{Binding}"/>
            </ViewCell>
          </DataTemplate>
        </ListView.ItemTemplate>

      </ListView>
      <Button FontSize="50"
        Text="+"
        CornerRadius="28"
        WidthRequest="56"
        HeightRequest="56"
        Padding="0"
        VerticalOptions="End"
        HorizontalOptions="End"
        Margin="25"
        Visual="Material" />
    </Grid>
  </ContentPage.Content>
</ContentPage>
